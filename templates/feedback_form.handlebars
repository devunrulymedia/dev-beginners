<script type='text/javascript'>
function sendFeedback() {
    var inputs = {
        "#entry_4": "entry.4.single",
        "#entry_5": "entry.5.single",
        "#entry_7": "entry.7.single",
        "input:radio[name='entry.9.group']:checked": "entry.9.group",
        "#entry_10": "entry.10.single"
    };

    var data = {
        pageNumber: 0,
        backupCache: undefined,
        submit: "Submit"
    };

    for (var i in inputs) {
        data[inputs[i]] = $(i).val();
    }

    var formUrl = 'https://docs.google.com/spreadsheet/formResponse?formkey=dDlQMVpNV1kxT2JfYWxBLW5aYVgyNGc6MQ&ifq'

    $.ajax({
        type: "POST",
        url: formUrl,
        data: data,
        success: function ajaxSuccess() {
            $('.ss-form').html('<p> Thank you! &lt;3<\/p>');
            $('.ss-form-container').attr('style', 'margin-top: -127px !important');
        },
        error: function ajaxFail() {
            $('.ss-form').html("<p> We broke something :'( Please try again later<\/p>");
        }
    });
}

$('.ss-form-container').addClass('hide');
setTimeout(function () {
    $('.ss-form-container').addClass('hide');

    $('#submitFeedback').click(function (e) {
        sendFeedback();
        return false;
    });

    $('#feedback').click(function (e) {
        $('.ss-form-container').toggleClass('hide');
        $('#main').toggleClass('fade');
        e.stopPropagation ? e.stopPropagation() : e.cancelBubble = true;
    });

    $('.close').click(function () {
        $('.ss-form-container').addClass('hide');
        $('#main').removeClass('fade');
    });
}, 10);
</script>